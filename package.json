{
  "name": "infotition-next-boilerplate",
  "version": "0.1.0",
  "description": "Boilerplate for Next.js projects with typescript, scass, eslint, prettier and custom express server support.",
  "main": "lib/index.js",
  "types": "lib",
  "bin": "./lib/index.js",
  "scripts": {
    "clean": "rimraf lib",
    "dev": "npm run clean&& tsc -p .&& cd lib&& node index.js",
    "start": "tsc -p .&& cd lib&& node index.js",
    "build": "tsc -p ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Infotition/infotition-next-boilerplate.git"
  },
  "keywords": [
    "nextjs",
    "boilderplate",
    "express",
    "eslint",
    "prettier",
    "scass"
  ],
  "author": "Infotition",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3",
    "eslint": "^7.18.0",
    "prettier": "^2.2.1",
    "@types/node": "^14.14.25",
    "@typescript-eslint/eslint-plugin": "^4.14.1",
    "@typescript-eslint/parser": "^4.14.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1"
  }
}
